<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="299.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="healthcaresystem.MainSystemController">
   <children>
      <Button fx:id="popUpCancelButton" layoutX="360.0" layoutY="177.0" mnemonicParsing="false" onAction="#cancelClick" prefHeight="52.0" prefWidth="121.0" style="-fx-background-radius: 0;" text="Cancel">
         <font>
            <Font name="SansSerif" size="16.0" />
         </font>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </Button>
      <Label fx:id="popUpLabel" alignment="CENTER" layoutY="-1.0" prefHeight="98.0" prefWidth="600.0" style="-fx-background-color: cecece;" text="Are you sure you want to log out?" textAlignment="CENTER">
         <font>
            <Font name="SansSerif" size="30.0" />
         </font>
      </Label>
      <Button fx:id="popUpLogOutButton" layoutX="117.0" layoutY="177.0" mnemonicParsing="false" onAction="#logOutClick" prefHeight="52.0" prefWidth="121.0" style="-fx-background-radius: 0;" text="Log Out">
         <font>
            <Font name="SansSerif" size="16.0" />
         </font>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </Button>
   </children>
</AnchorPane>
