public class Doctor extends Actor{
	String name;
	schedule schedule;
	int earned;
	//basic constructor, defaults to a test case
	public Doctor()
	{
		this.name = "Test Doctor";
		this.schedule = new schedule();
		this.clearance = 3;
		this.password = "test";
		this.earned = 0;
	}
	public Doctor(String name, schedule schedule, String password)
	{
		this.clearance = 3;
		this.name = name;
		this.schedule = new schedule();
		this.password = password;
		this.earned = 0;
	}
	
	public void updateEarned(int newEarned)
	{
		this.earned = this.earned + newEarned;
	}
}
