import java.util.ArrayList;

public class staff extends Actor {
	public staff()
	{
		this.name = "Test Staff";
		this.clearance = 1;
		this.password = "test";
	}
	
	public staff(String name, String password)
	{
		this.clearance = 1;
		this.name = name;
		this.password = password;
	}
	
	public void checkIn(ArrayList<patient> patient,String patName, String birth, staff staff)
	{
		for(patient pat : patient)
		{
			if(pat.name == patName)
			{
				if(staff.clearance >= 1)
				{
					if(pat.patientChart != null)
					{
						pat.patientChart.printChart();
						//take input if changes are necessary
						
					}
					else
					{
						//take input for patient info
						pat.patientChart = new patientChart();
					}
				}
				else 
				{
					//print something about not having clearance to do this.
				}
			}
		}
	}
}
