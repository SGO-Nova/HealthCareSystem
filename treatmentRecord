public class treatmentRecord {
	String date;
	String reason;
	String weight;
	String height;
	String bloodPressure;
	String treatment;
	String perscription;
	public treatmentRecord()
	{
		this.date = "";
		this.reason = "death";
		this.weight = "";
		this.height = "";
		this.bloodPressure = "";
		this.treatment = "";
		this.perscription = "";
	}
	
}
